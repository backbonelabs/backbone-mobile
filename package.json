{
  "name": "backbone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bugsnag": "node bugsnag.js --debug --release",
    "bugsnag-debug": "node bugsnag.js --debug",
    "bugsnag-release": "node bugsnag.js --release",
    "lint": "eslint index.* app/**",
    "prepush": "npm run lint && npm run test",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "setupFiles": [
      "./jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native)/"
    ]
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "bugsnag-react-native": "2.2.0",
    "class-autobind": "0.1.4",
    "color": "0.11.3",
    "lodash": "4.15.0",
    "moment": "2.17.1",
    "react": "15.4.2",
    "react-addons-shallow-compare": "15.6.0",
    "react-native": "0.42.3",
    "react-native-calendar": "0.10.0",
    "react-native-extended-stylesheet": "0.3.0",
    "react-native-fbsdk": "0.5.1",
    "react-native-fs": "2.1.0-rc.1",
    "react-native-keyboard-aware-scroll-view": "0.2.3",
    "react-native-material-kit": "0.4.1",
    "react-native-modal-datetime-picker": "4.9.0",
    "react-native-scrollable-tab-view": "0.7.4",
    "react-native-sensitive-info": "3.0.2",
    "react-native-slider": "0.10.0",
    "react-native-svg": "5.1.5",
    "react-native-vector-icons": "2.1.0",
    "react-native-video": "1.2.0",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "redux-async-action": "0.0.4",
    "redux-thunk": "2.1.0",
    "victory-native": "0.14.0"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-jest": "20.0.3",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "gradle-to-js": "0.2.5",
    "husky": "0.11.9",
    "jest": "20.0.4",
    "localenv": "0.2.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "15.4.2",
    "redux-mock-store": "1.2.3",
    "request": "2.81.0"
  }
}
